package com.example.tests;

import org.testng.annotations.Test;

public class EmptyContactCreation extends TestBase {
  @Test
  public void testContactCreation() throws Exception {
    openMainpage();
    initContactCreation();
    ContactData contact = new ContactData();
    contact.firstname		= "";
    contact.lastname		= "";
    contact.address			= "";
    contact.homephone		= "";
    contact.mobilephone		= "";
    contact.workphone		= "";
    contact.email			= "";
    contact.email2			= "";
    contact.bday			= "";
    contact.bmonth			= "";
    contact.byear			= "";
    contact.group			= "";
    contact.address			= "";
    contact.phone2			= "";
	fillContactForm(contact);
    submitContactCreation();
    returnToMainPage();
  }
}
